		{
			// AJAX Code To Submit Form.
			$.ajax({
			type: "POST",
			url: "getdata.php",
			data: {busqueda:busqueda},
			cache: false,
			success: function(result){
				//alert(result);
				$("#lista").html(result);
			}
		});
	}
	return false;
	});
});

